    "@types/dagre": "^0.7.48",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "4.0.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "cypress": "12.14.0",
