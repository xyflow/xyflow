{
  "name": "svelte-storybook",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "check": "svelte-check --tsconfig ./tsconfig.app.json && tsc -p tsconfig.node.json",
    "storybook": "storybook dev -p 6008",
    "build-storybook": "storybook build",
    "dev:compose": "concurrently -k \"pnpm -C ../react storybook -p 6007 --ci\" \"pnpm storybook\""
  },
  "dependencies": {
    "storybook-shared": "workspace:*"
  },
  "devDependencies": {
    "@storybook/addon-docs": "^9.1.5",
    "@storybook/addon-a11y": "^9.1.5",
    "@storybook/addon-svelte-csf": "^5.0.8",
    "@storybook/addon-vitest": "^9.1.5",
    "@storybook/svelte-vite": "^9.1.5",
    "@storybook/test": "^8.6.14",
    "@sveltejs/vite-plugin-svelte": "^6.1.1",
    "@testing-library/svelte": "^5.2.8",
    "@tsconfig/svelte": "^5.0.4",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@xyflow/svelte": "workspace:*",
    "playwright": "^1.55.0",
    "storybook": "^9.1.5",
    "svelte": "^5.38.1",
    "svelte-check": "^4.3.1",
    "typescript": "~5.8.3",
    "vite": "^7.1.2",
    "vitest": "^3.2.4"
  }
}
